<%= form_for [@user, @user.contacts.build] do |f| %>
  First Name: <%= f.text_field :f_name %><br>
  Last Name: <%= f.text_field :l_name %><br>
  Email: <%= f.text_field :email %><br>
  Home phone: <%= f.text_field :home_phone %><br>
  Mobile phone: <%= f.text_field :mobile_phone %><br>
  Work phone: <%= f.text_field :work_phone %><br>
  Street: <%= f.text_field :street %><br>
  City: <%= f.text_field :city %><br>
  State: <%= f.text_field :state %><br>
  Postal Code:<%= f.text_field :zip %><br><br>
  <%= f.submit %>

<% end %>